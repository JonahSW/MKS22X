MyLinkedList list1 = new MyLinkedList();
	list1.add(1);
	list1.add(2);
	list1.add(3);
	list1.add(4);
	list1.add(5);
	list1.toString();
	
	for(int node : list1){
	    System.out.print(node + ", ");
	}
	System.out.println();

